#include <stdio.h>

int main() {

	float a,b,c,d,e,f,m,n;

	printf("nhap 6 gia tri");

	scanf("%f %f %f %f %f %f", &a, &b ,&c,&d,&e,&f);

	m=(a*f/d-c);

	n=(a*e/d-b);

	if (n==0&&m!=0)

	printf("pt vo n");

	else if (m==0&&n==0)

	printf("pt vo so n");

	else

	printf("(%0.5f,%0.5f)",(c-b*m/n)/a,m/n);

	return 0;

	}
